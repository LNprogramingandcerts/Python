meal = ["egg", "bacon", "spam", "sausages"]

for item in meal:
    if item == 'spam':
        nasty_food_item = item
        break
else:
    nasty_food_item = item = ''
    print("I'll have a pplate of that, then, please")
if nasty_food_item:
        print("Can't I have anything without spam in it")
